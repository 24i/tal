{
  "modules": {
    "base": "antie/devices/ps3",
    "modifiers": [
      "antie/devices/media/ps3native",
      "antie/devices/anim/scrolloffset",
      "antie/devices/data/json2",
      "antie/devices/net/default"
    ]
  },
  "mediasets": {
    "tv": "stb-all-h264",
    "radio": "stb-aac"
  },
  "streaming": {
    "video": {
      "mediaSelectorAPI": "js",
      "mediaSelectorURI": "https://ipsecure.stage.bbc.co.uk/mediaselector/4/js/stream/%vpid%",
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "protocols": [
            "http"
          ],
          "encodings": [
            "h264"
          ]
        }
      ]
    },
    "audio": {
      "mediaSelectorAPI": "js",
      "mediaSelectorURI": "https://ipsecure.stage.bbc.co.uk/mediaselector/4/js/stream/%vpid%/iplayer_stb_uk_stream_aac_concrete",
      "mediaURIFormat": "%href%",
      "supported": [
        {
          "protocols": [
            "http"
          ],
          "encodings": [
            "aac"
          ]
        }
      ]
    }
  },
  "input": {
    "map": {
      "alpha": [
        65,
        90
      ],
      "numeric": [
        48,
        57
      ],
      "37": "LEFT",
      "38": "UP",
      "39": "RIGHT",
      "40": "DOWN",
      "32": "ENTER",
      "128": "PLAY",
      "129": "STOP",
      "130": "PAUSE",
      "126": "FAST_FWD",
      "127": "REWIND",
      "8": "BACK",
      "113": "BACK_SPACE",
      "131": "EXIT"
    }
  },
  "accessibility": {
    "captions": {
      "supported": [
        "application/ttaf+xml"
      ]
    }
  },
  "layouts": [
    {
      "width": 1280,
      "height": 720,
      "module": "%application%/appui/layouts/720p",
      "classes": [
        "browserdevice720p",
        "ps3"
      ]
    }
  ]
}
