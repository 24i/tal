{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "curly": 2,
        "no-console": 0,
        "no-constant-condition":0,
        "no-empty":0,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-new-wrappers": 2,
        "brace-style": 2,
        "no-eval": 2
    },
    "globals": {
        "require": true,
        "antie": true,
        "ActiveXObject" :true
    },
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "plugins": [
        "jasmine",
        "requirejs"
    ]
}
